{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "env": {
      "API_SECRET": "@api_secret"
    },
    "build": {
      "env": {
        "API_SECRET": "@api_secret"
      }
    }
  }
  